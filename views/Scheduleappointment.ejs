async function showOverlay(date) {
    const selectedDateElement = document.getElementById('selectedDate');
    const appointmentTimesContainer = document.getElementById('appointmentTimes');

    // Display selected date
    selectedDateElement.textContent = date;

    // Fetch appointment times for the selected date (you can replace this with your own logic)
    const appointmentTimes = await getAppointmentTimesForDate(date);

    // Clear previous appointment times
    appointmentTimesContainer.innerHTML = '';

    // Display appointment times
    appointmentTimes.forEach(time => {
        const timeElement = createTimeElement(time,date);
        appointmentTimesContainer.appendChild(timeElement);
    });

    // Create button to add more time stamps
    const addButton = document.createElement('button');
    addButton.textContent = 'Add Time Stamp';
    addButton.classList.add('btn-add');
    addButton.addEventListener('click', () => addNewTime(date));

    appointmentTimesContainer.appendChild(addButton);

    // Show overlay
    const overlay = document.getElementById('overlay');
    overlay.style.display = 'block';
}

function createTimeElement(time,date) {
    const timeContainer = document.createElement('div');
    timeContainer.classList.add('timestamp-container');
    const timeElement = document.createElement('div');
    timeElement.textContent = time.STARTTIME + ' - ' + time.ENDTIME + ' (Capacity: ' + time.CAPACITY + ')' + ' (Patient: ' + time.PATIENT + ')'
    if(time.PATIENT == 0){
          // Create update button

